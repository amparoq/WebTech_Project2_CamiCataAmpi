<h1>Edit Executive</h1>
<%= form_with(model: @executive, url: executive_path(@executive), method: :patch) do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div class="field">
    <%= form.label :phone %>
    <%= form.text_field :phone %>
  </div>
  <div class="actions">
    <%= form.submit 'Update Executive' %>
  </div>
<% end %>
<%= link_to 'Back to Executive', executive_path(@executive) %>
